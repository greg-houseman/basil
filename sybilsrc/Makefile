preserve::
	@for flag in ${MAKEFLAGS} ''; do \
	  case "$$flag" in *=*) ;; --*) ;; *[ik]*) set +e;; esac; done; \
	for i in $(SUBDIRS) ;\
	do \
		echo "making" preserve "in $(CURRENT_DIR)/$$i..."; \
		$(MAKE) -C $$i $(MFLAGS) $(PARALLELMFLAGS)  preserve; \
	done

