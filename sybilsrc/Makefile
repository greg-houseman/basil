                             $(EXTRA_LOAD_FLAGS)
cleandir::
	$(RM) sybil

preserve::
	if [ -f $(BINDIR)/sybil ]; then           \
	$(MV) $(BINDIR)/sybil $(BINDIR)/OLDsybil; \
	fi

install:: sybil
	@if [ -d $(DESTDIR)$(BINDIR) ]; then \
		set +x; \
	else \
		if [ -h $(DESTDIR)$(BINDIR) ]; then \
			(set -x; rm -f $(DESTDIR)$(BINDIR)); \
		fi; \
		(set -x; $(MKDIRHIER) $(DESTDIR)$(BINDIR)); \
	fi
	$(INSTALL) $(INSTALLFLAGS) $(INSTPGMFLAGS)  sybil $(DESTDIR)$(BINDIR)/sybil

install.man::
	@if [ -d $(MANDIR) ]; then \
		set +x; \
	else \
		if [ -h $(MANDIR) ]; then \
			(set -x; rm -f $(MANDIR)); \
		fi; \
		(set -x; $(MKDIRHIER) $(MANDIR)); \
	fi
	$(RM) $(MANDIR)/sybil.l
	$(LN) $(DOCSDIR)/sybil.l $(MANDIR)/sybil.l

